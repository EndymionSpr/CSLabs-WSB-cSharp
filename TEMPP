using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication1
{
    class Program
    {
        static bool CzyPierwsza(uint a)
        {
            if (a < 2) return false;
            else if (a==2) return true;
            for (uint i = a - 1; i > 1; i--)
            {
                else if (a % i == 0) return false;
                else if (i == 2) return true;
            }
        }
        static uint ZwracaPierwszą(uint a)
        {
            for (uint i = a - 1; i > 1; i--)
            {
                if (CzyPierwsza(i)) return i;

            } }
        static void Main(string[] args)
        {
            
            Console.WriteLine("Podaj przedział: ");
            string str = Console.ReadLine();
            uint a = uint.Parse(str);
            str = Console.ReadLine();
            uint b = uint.Parse(str);

            for (uint i=a;i<= b;i++)
            {
                if (i % 2 != 0) continue;
                if (CzyPierwsza(i - ZwracaPierwszą(i))) Console.WriteLine("{0} = {1} + {2}", +i, +ZwracaPierwszą(i), +i - ZwracaPierwszą(i);)
                else (CzyPierwsza(i - ZwracaPierwszą(i)))


            }
        }
    }
}
